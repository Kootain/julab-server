{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {},
  "files": {},
  "final": {
    "loopback#urlNotFound": {}
  },
  "files": {
    "loopback#static": [
      {"paths": "/", "params": "$!../src"},
      {"paths": "/bower_components", "params": "$!../bower_components"},
      {"paths": "/node_modules", "params": "$!../node_modules"}
    ]
  },
  "final:after": {
    "errorhandler": {}
  }
}
