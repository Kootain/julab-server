{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {},
  "files": {
    "loopback#static": [
      {
        "params": "$!../app", 
        "paths": "/"
      },
      {
        "params": "$!../bower_components",
        "paths": "/bower_components"
      },
      {
        "params": "$!../node_modules",
        "paths": "/node_modules"
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "errorhandler": {}
  }
}
